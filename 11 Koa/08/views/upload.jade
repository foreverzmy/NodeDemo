
extends layout

block content
  h1= title
  p Upload a photo to your account below.
  form(method='post' enctype='multipart/form-data')
    p
      input(type='text',name='photo[name]',placeholder="Name")
    p
      input(type='file',name='photo[image]')
    p
      input(type='submit',value="Upload")
      
  

